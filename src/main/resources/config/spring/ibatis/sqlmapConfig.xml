<sqlMapConfig>
	<!-- 服务器信息 -->
	<sqlMap resource="com/junyou/bus/serverinfo/sqlmap/ServerInfo.xml"/>
	<!-- 角色信息 -->
 	<sqlMap resource="com/junyou/bus/role/sqlmap/UserRole.xml"/>
 	<sqlMap resource="com/junyou/bus/account/sqlmap/RoleAccount.xml"/>
	<!-- 充值 -->
